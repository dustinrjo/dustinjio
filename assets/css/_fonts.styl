@font-face
  font-family 'brand-serif'
  font-weight normal
  font-style normal
  src url('fonts/GoudyStM-Italic-webfont.eot')
  src url('fonts/GoudyStM-Italic-webfont.eot?#iefix') format('embedded-opentype'),
      url('fonts/GoudyStM-Italic-webfont.woff') format('woff'),
      url('fonts/GoudyStM-Italic-webfont.ttf') format('truetype'),
      url('fonts/GoudyStM-Italic-webfont.svg?-ezp7rl#icomoon') format('svg')


@font-face
  font-family 'icomoon'
  src url('fonts/icomoon.eot?-ezp7rl')
  src url('fonts/icomoon.eot?#iefix-ezp7rl') format('embedded-opentype'),
  url('fonts/icomoon.woff?-ezp7rl') format('woff'),
  url('fonts/icomoon.ttf?-ezp7rl') format('truetype'),
  url('fonts/icomoon.svg?-ezp7rl#icomoon') format('svg')
  font-weight normal
  font-style normal

[class^="icon-"], [class*=" icon-"]
